# Indiquem amb quin tipus de llenguatge tractem:
#!/bin/bash

# Algunes de les variables globals:
echo "Nom arxiu script : $0"
echo "Total arguments llegits : $#"
echo "Llista arguments : $*"
echo "Llista arguments : $@"

# Visualiztem arguments de la Shell
echo "Llista arguments SHELL : $@"

# Captar al valor retornat per la darrera funci√≥ o comanda
echo "Valor retornat : $?"

# Mostrar al PID:
echo "PID Shell actual: $$"

